{
    "name": "barryvdh/laravel-elfinder",
    "keywords": ["laravel", "elfinder", "tinymce", "filebrowser"],
    "description": "A Laravel Package to integrate elFinder 2",
    "license": "MIT",
    "authors": [
        {
            "name": "Barry vd. Heuvel",
            "email": "barryvdh@gmail.com"
        }
    ],
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/redalpha01/elfinder-builds"
		}
	],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.x",
		"barryvdh/elfinder-builds": "2.1.1",
        "barryvdh/elfinder-flysystem-driver": "0.1.x@dev"
    },
    "autoload": {
        "psr-4": {
            "Barryvdh\\Elfinder\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    },
    "minimum-stability": "dev"
}
